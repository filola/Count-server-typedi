{
  "config": {
    "target": "http://localhost:3000",
    "phases": [
      {
        "duration": 1000,
        "arrivalRate": 22
      }
    ],
    "defaults": {
      "headers": {
        "User-Agent": "Artillery"
      }
    },
    "payload": {
      "path": "./post900.csv",
      "fields": ["userId"]
    }
  },
  "scenarios": [
    {
      "name": "Joining user",
      "flow": [
        {
          "post": {
            "url": "/"
          }
        },
        {
          "post": {
            "url": "/EnterCheck",
            "json": {
              "userId": "{{userId}}"
            },
            "count": 100
          }
        },
        {
          "post": {
            "url": "/disconnect"
          }
        }
      ]
    }
  ]
}
