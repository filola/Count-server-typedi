{
  "config": {
    "target": "http://localhost:3000",
    "phases": [
      {
        "duration": 10,
        "arrivalRate": 1
      }
    ],
    "defaults": {
      "headers": {
        "User-Agent": "Artillery"
      }
    },
    "payload": {
      "path": "./abc.csv",
      "fields": [
        "userId"
      ]
    }
  },
  "scenarios": [
    {
      "name": "Joining user",
      "flow": [
        {
          "loop": {
            "post": {
              "url": "/EnterCheck",
              "json": {
                "userId": "{{userId}}"
              }
            },
            "think": 10
          }
        }
      ]
    }
  ]
}